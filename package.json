{
    "name": "webdriverio-mocha-appium-momentumsuite",
    "version": "0.1.0",
    "readme": "WebdriverIO Integration with local or [MomentumSuite](https://www.momentumsuite.com) real mobile farm devices",
    "description": "Boilerplate project to run MOBILE Test Automation with WebdriverIO v7, Mocha, Appium, Allure reporting and Momentum Suite cloud device farm support",
    "scripts": {
        "test": "npm run android-first",
        "android-first": "./node_modules/.bin/wdio examples/getting-started/android.first.conf.js",
        "android-parallel": "./node_modules/.bin/wdio examples/parallel-test/android.parallel.conf.js",
        "android-local": "./node_modules/.bin/wdio examples/local-test/android.local.conf.js",
        "android-pom": "./node_modules/.bin/wdio examples/pom-test/android.pom.conf.js",
        "android-cucumber": "./node_modules/.bin/wdio examples/cucumber-test/android.cucumber.conf.js",
        "android-multiple": "./node_modules/.bin/wdio examples/multiple-test/android.multiple.conf.js",
        "android-chrome": "./node_modules/.bin/wdio examples/chrome-test/android.chrome.conf.js",
        "ios-first": "./node_modules/.bin/wdio examples/getting-started/ios.first.conf.js",
        "ios-parallel": "./node_modules/.bin/wdio examples/parallel-test/ios.parallel.conf.js",
        "ios-local": "./node_modules/.bin/wdio examples/local-test/ios.local.conf.js",
        "ios-pom": "./node_modules/.bin/wdio examples/pom-test/ios.pom.conf.js",
        "ios-cucumber": "./node_modules/.bin/wdio examples/cucumber-test/ios.cucumber.conf.js",
        "ios-multiple": "./node_modules/.bin/wdio examples/multiple-test/ios.multiple.conf.js",
        "ios-safari": "./node_modules/.bin/wdio examples/safari-test/ios.safari.conf.js"
    },
    "dependencies": {
        "@badisi/wdio-harness": "^2.0.1",
        "@wdio/allure-reporter": "^8.3.5",
        "@wdio/appium-service": "^8.3.5",
        "@wdio/cli": "^8.3.5",
        "@wdio/cucumber-framework": "^8.3.0",
        "@wdio/local-runner": "^8.3.5",
        "@wdio/mocha-framework": "^8.3.0",
        "@wdio/spec-reporter": "^8.3.0",
        "allure-commandline": "^2.17.2",
        "chai": "^4.3.6",
        "chromedriver": "^101.0.0",
        "mocha-allure-reporter": "^1.4.0",
        "wdio-chromedriver-service": "^8.0.1",
        "wdio-wait-for": "^2.2.6",
        "webdriverio": "^8.3.5"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/momentumsuite/webdriverio-mocha-appium-momentumsuite.git"
    },
    "keywords": [
        "webdriverio",
        "wdio",
        "momentumsuite",
        "mobven",
        "appium",
        "selenium",
        "tests",
        "native app",
        "hybrid",
        "android",
        "ios"
    ],
    "bugs": {
        "url": "https://github.com/momentumsuite/webdriverio-mocha-appium-momentumsuite/issues"
    },
    "homepage": "https://github.com/momentumsuite/webdriverio-mocha-appium-momentumsuite#readme"
}
